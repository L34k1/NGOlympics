<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.isep.tentative.gestDisController">

    <children>
        <TableView fx:id="table" layoutX="225.0" layoutY="100.0" prefHeight="200.0" prefWidth="150.0">
            <columns>
                <TableColumn fx:id="idCol" prefWidth="75.0" text="id" />
                <TableColumn fx:id="nameCol" prefWidth="75.0" text="nom" />
            </columns>
        </TableView>
        <Button fx:id="bRetour" layoutX="70.0" layoutY="45.0" mnemonicParsing="false" onAction="#onbRetourMainMenuButtonClick" text="Retour au menu principal" />
        <Button fx:id="bAdd" layoutX="70.0" layoutY="336.0" mnemonicParsing="false" onAction="#onbAddDisButtonClick" text="Ajouter" />
        <Button fx:id="bMod" layoutX="274.0" layoutY="336.0" mnemonicParsing="false" onAction="#onbModDisButtonClick" text="Modifier" />
        <Button fx:id="bRem" layoutX="488.0" layoutY="336.0" mnemonicParsing="false" onAction="#onbRemDisButtonClick" text="Supprimer" />
    </children>
</AnchorPane>
